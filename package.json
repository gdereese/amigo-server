{
  "name": "amigo-server",
  "version": "0.1.0",
  "description": "Server that allows you to create and manage a graph of users linked to others users as friends.",
  "keywords": [
    "nodejs",
    "typescript",
    "restify",
    "friends"
  ],
  "author": "Gary DeReese Jr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdereese/amigo-server.git"
  },
  "bugs": {
    "url": "https://github.com/gdereese/amigo-server/issues"
  },
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "build": "npm run-script lint && tsc",
    "license-check": "abogado",
    "lint": "tslint --project tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run-script lint && tsc",
    "preversion": "npm test",
    "version": "npm run-script build",
    "test": "jasmine-ts test/**/*.spec.ts"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "@types/lodash": "4.14.105",
    "@types/node": "9.4.7",
    "@types/restify": "5.0.7",
    "abogado": "1.0.4",
    "jasmine-ts": "0.2.1",
    "prettier": "1.11.1",
    "restify-clients": "2.0.0",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.10.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "commander": "2.15.0",
    "lodash": "4.17.5",
    "lodash-query": "1.4.2",
    "moment": "2.21.0",
    "restify": "6.3.4",
    "restify-errors": "6.0.0"
  }
}
