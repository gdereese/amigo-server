{
  "name": "amigo-server",
  "version": "0.1.0",
  "description":
    "Server that allows you to create and manage a graph of users linked to others users as friends.",
  "keywords": ["nodejs", "restify", "api", "friends"],
  "author": "Gary DeReese Jr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdereese/amigo-server.git"
  },
  "bugs": {
    "url": "https://github.com/gdereese/amigo-server/issues"
  },
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": ["dist/**/*.js"],
  "scripts": {
    "build": "npm run-script lint && babel src -d dist",
    "license-check": "abogado",
    "lint": "eslint src/** test/**",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run-script build",
    "preversion": "npm test",
    "version": "npm run-script build",
    "test": "jasmine test/**/*.spec.js"
  },
  "devDependencies": {
    "abogado": "1.0.4",
    "babel-cli": "6.26.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-jasmine": "2.9.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-standard": "3.0.1",
    "jasmine": "3.1.0",
    "prettier": "1.11.1",
    "restify-clients": "2.0.0"
  },
  "dependencies": {
    "commander": "2.15.0",
    "fortune": "5.4.2",
    "lodash": "4.17.5",
    "lodash-query": "1.4.2",
    "moment": "2.21.0",
    "restify": "6.3.4",
    "restify-errors": "6.0.0"
  }
}
